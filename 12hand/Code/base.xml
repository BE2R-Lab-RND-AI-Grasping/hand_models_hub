<mujoco>

    <option timestep="1e-5"/>
    <option gravity="0 0 0"/>
    <option integrator="implicitfast"/>
    <!--Euler, RK4, implicit, implicitfast-->

    <asset>
        <texture type="skybox" builtin="gradient" rgb1="1 1 1" rgb2="1 1 1" width="265" height="256"/>
        <texture name="grid" type="2d" builtin="checker" rgb1="0.1 0.2 0.3" rgb2="0.2 0.3 0.4" width="300" height="300"/>
        <material name="grid" texture="grid" texrepeat="10 10" reflectance="0.2"/>
    </asset>

    <default class="main"> 
        <joint damping="0" armature="0.01" frictionloss="0.01"/>
        <geom condim="4" contype="1" conaffinity="2" friction="0.9 0.2 0.2" solref="0.001"/>
        <motor ctrllimited="true"/>
        <position ctrllimited="true" dampratio="0" kp="1000000"/>
        <equality solref="0.000001 30"/>
    </default>

    <worldbody>
        <light pos="0 0 10"/>

        
    </worldbody>


</mujoco>
